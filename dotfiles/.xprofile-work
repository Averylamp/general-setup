#!/bin/bash
xrandr --output DP-1 --off
xrandr --output DP-2 --off
xrandr --output DP-3 --off
xrandr --output DP-4 --off
xrandr --output DP-5 --off
xrandr --output DP-0 --mode 5120x2160 --pos 0x600 --rate 60.00 --rotate normal
xrandr --output HDMI-0 --mode 3840x2160 --pos 5120x0 --rate 60.00 --rotate right
